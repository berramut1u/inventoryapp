{
  "compilerOptions": {
    "jsx": "react-jsx",
    "lib": [ "DOM", "ESNext" ],
    "types": [ "react", "react-dom" ],
    "module": "ESNext",
    "moduleResolution": "Node",
    "target": "ESNext",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true
  }
}
